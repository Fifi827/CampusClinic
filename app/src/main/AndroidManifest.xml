<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cut.campusclinic">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".AppClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BookAppointment"></activity>
        <activity android:name=".UpdateProfile" />
        <activity android:name=".FullRecord" />
        <activity android:name=".RecordList" />
        <activity android:name=".DrApprovedAppointments" />
        <activity android:name=".ApproveAppointment" />
        <activity android:name=".DoctorAppointment" />
        <activity android:name=".DoctorHome" />
        <activity android:name=".AppointmentChoice" />
        <activity android:name=".AppointmentList" />
        <activity android:name=".ConsultActivity" />
        <activity android:name=".AppointmentActivity" />
        <activity android:name=".PatientHome" />
        <activity android:name=".AdminHome" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>